{
  "name": "serial-studio-vscode",
  "displayName": "Serial Studio",
  "description": "A powerful VSCode extension for serial port data visualization and analysis, based on Serial Studio",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.74.0",
    "node": ">=18.0.0"
  },
  "categories": [
    "Visualization",
    "Data Science",
    "Other"
  ],
  "keywords": [
    "serial",
    "port",
    "data",
    "visualization",
    "charts",
    "sensors",
    "iot",
    "dashboard"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "serialStudio.openDashboard",
        "title": "Open Serial Studio Dashboard",
        "category": "Serial Studio"
      },
      {
        "command": "serialStudio.connectDevice",
        "title": "Connect to Device",
        "category": "Serial Studio"
      },
      {
        "command": "serialStudio.disconnectDevice",
        "title": "Disconnect Device",
        "category": "Serial Studio"
      },
      {
        "command": "serialStudio.openProjectEditor",
        "title": "Open Project Editor",
        "category": "Serial Studio"
      }
    ],
    "viewsWelcome": [
      {
        "view": "serialStudio.devices",
        "contents": "No devices connected.\n[Connect Device](command:serialStudio.connectDevice)"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "serialStudio.devices",
          "name": "Serial Studio Devices",
          "when": "serialStudio.enabled"
        }
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Serial Studio",
      "properties": {
        "serialStudio.performance.updateFrequency": {
          "type": "number",
          "default": 20,
          "description": "Data update frequency in Hz (frames per second)"
        },
        "serialStudio.performance.maxDataPoints": {
          "type": "number",
          "default": 10000,
          "description": "Maximum data points to retain in memory"
        },
        "serialStudio.communication.autoReconnect": {
          "type": "boolean",
          "default": true,
          "description": "Automatically reconnect to devices on connection loss"
        },
        "serialStudio.ui.theme": {
          "type": "string",
          "enum": [
            "auto",
            "light",
            "dark"
          ],
          "default": "auto",
          "description": "UI theme preference"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "prettier:check": "prettier --check \"src/**/*.{ts,vue,json,md}\"",
    "prettier:fix": "prettier --write \"src/**/*.{ts,vue,json,md}\"",
    "type-check": "tsc --noEmit",
    "test:type-check": "tsc --project tsconfig.test.json --noEmit",
    "test:type-check-watch": "tsc --project tsconfig.test.json --noEmit --watch",
    "test": "vitest --config utest/vitest.config.mjs",
    "test:unit": "vitest --config utest/vitest.config.mjs --run",
    "test:coverage": "vitest --config utest/vitest.config.mjs --coverage",
    "test:coverage:full": "vitest --config utest/vitest.config.mjs --coverage --run",
    "test:performance": "vitest --config utest/vitest.config.mjs --run --reporter=verbose **/*.bench.ts",
    "test:integration": "vitest --config utest/vitest.config.mjs --run utest/integration",
    "test:memory-leaks": "node --expose-gc ./scripts/memory-leak-detector.js",
    "test:watch": "vitest --config utest/vitest.config.mjs",
    "coverage:report": "vitest --config utest/vitest.config.mjs --coverage --reporter=lcov",
    "coverage:check": "vitest --config utest/vitest.config.mjs --coverage.thresholds.autoUpdate=false --coverage --run",
    "coverage:optimized": "npx vitest --config=vitest.coverage.config.mjs --coverage --run final-coverage-summary.test.mjs",
    "test:coverage-check": "./run-coverage-tests.sh",
    "test:optimized": "npm run test:checksum && npm run test:circularbuffer && npm run test:datatransformer",
    "test:checksum": "npx vitest --config=vitest.coverage.config.mjs --coverage --run checksum-coverage.test.mjs",
    "test:circularbuffer": "npx vitest --config=vitest.coverage.config.mjs --coverage --run circularbuffer-coverage.test.mjs",
    "test:datatransformer": "npx vitest --config=vitest.coverage.config.mjs --coverage --run datatransformer-coverage.test.mjs",
    "security:scan": "npm audit --audit-level high",
    "vsce:package": "vsce package",
    "benchmark": "node scripts/performance-benchmark.js"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "@types/vscode": "^1.74.0",
    "@types/vue": "^1.0.31",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-istanbul": "^1.6.1",
    "@vitest/coverage-v8": "^1.0.0",
    "@vscode/vsce": "^3.6.0",
    "@vue/test-utils": "^2.4.0",
    "c8": "^10.1.3",
    "chartjs-adapter-date-fns": "^3.0.0",
    "eslint": "^8.28.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.0.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.4",
    "vitest": "^1.0.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "@abandonware/noble": "^1.9.2-15",
    "@msgpack/msgpack": "^3.0.0",
    "@types/d3": "^7.4.3",
    "@types/leaflet": "^1.9.20",
    "@types/mqtt": "^0.0.34",
    "@types/three": "^0.178.1",
    "ajv": "^8.12.0",
    "chart.js": "^4.5.0",
    "comlink": "^4.4.0",
    "csv-parser": "^3.0.0",
    "d3": "^7.9.0",
    "element-plus": "^2.10.4",
    "exceljs": "^4.3.0",
    "fft.js": "^4.0.4",
    "jszip": "^3.10.0",
    "leaflet": "^1.9.4",
    "lz4js": "^0.2.0",
    "ml-fft": "^1.3.5",
    "mqtt": "^5.14.0",
    "pinia": "^2.3.1",
    "serialport": "^12.0.0",
    "three": "^0.178.0",
    "vm2": "^3.9.0",
    "vue": "^3.5.18",
    "ws": "^8.14.0",
    "xml2js": "^0.6.0"
  },
  "activationEvents": [
    "onCommand:serialStudio.openDashboard",
    "onStartupFinished"
  ],
  "extensionDependencies": [],
  "publisher": "serial-studio",
  "repository": {
    "type": "git",
    "url": "https://github.com/Serial-Studio/vscode-extension"
  },
  "bugs": {
    "url": "https://github.com/Serial-Studio/vscode-extension/issues"
  },
  "homepage": "https://serial-studio.com",
  "license": "GPL-3.0-only",
  "icon": "assets/icon.png"
}
